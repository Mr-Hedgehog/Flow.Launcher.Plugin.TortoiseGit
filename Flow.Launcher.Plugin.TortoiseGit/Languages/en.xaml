<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--  Plugin Info  -->
    <system:String x:Key="flowlauncher_plugin_tortoisegit_plugin_name">TortoiseGit</system:String>
    <system:String x:Key="flowlauncher_plugin_tortoisegit_plugin_description">TortoiseGit shortcuts</system:String>

    <!--  Settings  -->
    <system:String x:Key="flowlauncher_plugin_tortoisegit_searchGitReposIn">Search repositories in:</system:String>
    <system:String x:Key="flowlauncher_plugin_tortoisegit_gitRepositoryName">Name</system:String>
    <system:String x:Key="flowlauncher_plugin_tortoisegit_gitRepositoryPath">Path</system:String>
    <system:String x:Key="flowlauncher_plugin_tortoisegit_addGitRepository">Add</system:String>
    <system:String x:Key="flowlauncher_plugin_tortoisegit_deleteGitRepository">Delete</system:String>
    <system:String x:Key="flowlauncher_plugin_tortoisegit_gitRepositorySetting">Git repository settings</system:String>
</ResourceDictionary>